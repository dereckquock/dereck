{
  "name": "dereckquock",
  "description": "Dereck's personal site",
  "version": "1.0.0",
  "author": "Dereck Quock <djquock@gmail.com>",
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mdx-js/mdx": "^1",
    "@mdx-js/react": "^1",
    "@reach/dialog": "^0.17.0",
    "@reach/visually-hidden": "^0.17.0",
    "facepaint": "^1.2.1",
    "gatsby": "^4.13.1",
    "gatsby-image": "^3.10.0",
    "gatsby-plugin-emotion": "^7.13.0",
    "gatsby-plugin-manifest": "^4.13.0",
    "gatsby-plugin-mdx": "^3.13.0",
    "gatsby-plugin-pnpm": "^1.2.10",
    "gatsby-plugin-react-helmet": "^5.13.0",
    "gatsby-plugin-sharp": "^4.13.0",
    "gatsby-remark-autolink-headers": "^5.13.0",
    "gatsby-remark-images": "^6.13.0",
    "gatsby-source-filesystem": "^4.13.0",
    "gatsby-transformer-sharp": "^4.13.0",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "keywords": [
    "gatsby",
    "mdx",
    "dereck",
    "dereck quock"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --log-pages",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,mdx}\"",
    "lint-staged": "lint-staged",
    "lint": "eslint . --ext js,mdx",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prebuild": "npx pnpm install -r --store=node_modules/.pnpm-store || echo skipping pnpm install",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "start": "gatsby develop"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.7.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mdx": "^1.17.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dereckquock/dereck"
  },
  "lint-staged": {
    "*.{js,mdx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
